<div class="jumbotron"><h2 align="center"> Sign up </h2></div>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<div class="col-md-4" align="center"></div>
   <div class="col-md-4">
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email,placeholder: "Email Id",class: "form-control",autofocus: true %>
  </div>
<div class="field">
    <%= f.label :rollno %><br/>
    <%= f.text_field :regno,placeholder: "Roll_No",class: "form-control",autofocus:true %>
  </div>
<% end%>
</div>
</div>